<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record model="ir.cron" id="workflow_3_blog_page_template_cron">
        <field name="name">Workflow 3 Blog Page Template Schedular</field>
        <field name="model_id" ref="website_blog.model_blog_post"/>
        <field name="state">code</field>
        <field name="code">model.workflow_3_blog_page_template_cron()</field>
        <field name="interval_number">1</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 00:00:00')"/>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">False</field>
    </record>
</odoo>
